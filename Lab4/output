FUNCTION main :
DEC t0 12
t3 := &t0
t4 := #0
t5 := t4 * #4
t1 := t3 + t5
t2 := #0
*t1 := t2
t8 := &t0
t9 := #1
t10 := t9 * #4
t6 := t8 + t10
t7 := #1
*t6 := t7
t13 := &t0
t14 := #2
t15 := t14 * #4
t11 := t13 + t15
t12 := #2
*t11 := t12
READ t17
t16 := t17
t19 := &t0
t20 := t16
t21 := t20 * #4
t22 := t19 + t21
t18 := *t22
WRITE t18
